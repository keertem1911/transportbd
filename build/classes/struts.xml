<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

	<constant name="struts.devMode" value="true" />		<!-- 开发模式 -->
	<constant name="struts.action.extension" value="action" />		<!-- 后缀名为.action -->
 	<constant name="struts.i18n.encodeing" value="utf-8"/>
	<package name="hello" namespace="/" extends="struts-default">
	 	<action name="chart_*" method="{1}" class="com.transportbd.action.ChartAction"></action>
		<action name="data_*" method="{1}" class="com.transportbd.action.DataAction"></action>
		
	</package>
	
</struts>